{
  "name": "graphql-fastify-typescript-microservice-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --trace-warnings --poll src/server.ts",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix",
    "build": "tsc -p .",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "apollo-server": "^3.1.2",
    "apollo-server-express": "^3.1.2",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "graphql": "^15.5.1",
    "jest": "^27.0.6",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.2.0",
    "type-graphql": "^1.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/introspection": "2.0.0",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-document-nodes": "2.0.0",
    "@graphql-codegen/typescript-resolvers": "2.0.0",
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
